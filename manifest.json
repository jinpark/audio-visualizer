{
  "manifest_version": 2,
  "name": "SoundCloud Visualizer",
  "version": "0.0.2",
  "permissions": [
    "tabCapture",
    "activeTab", 
    "alarms"
  ],
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {   
      "256": "sound.png"
    },
    "default_title": "Audio Visualizer", 
    "default_popup": "popup.html"        
  }
}
